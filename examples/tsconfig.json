{
    "compileOnSave": true,
    "compilerOptions": {
        "module": "commonjs",
        "alwaysStrict": true,
        "lib": [
            "es2017",
            "dom"
        ],
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noFallthroughCasesInSwitch": true,
        "outDir": "examples",
        "pretty": true,
        "strictNullChecks": true,
        "inlineSources": true,
        "inlineSourceMap": true,
        "target": "es2017",
        "rootDir": ".",
        "typeRoots": ["../typings", "../node_modules/@types"]
    },
    "include": [
        "*.ts"
    ],
    "exclude": []
}

